//https://www.dotnetperls.com/split
// This function split is to split the string


using System;

class Program
{
    static void Main()
    {
        string data = "there is a cat"; // a string
        // Split string on spaces (this will separate all the words).
        string[] words = data.Split(' '); //for every ' '(space) split the string into different strings
        foreach (string word in words)
        {
            Console.WriteLine("WORD: " + word);
        }
    }
}

Output

WORD: there
WORD: is
WORD: a
WORD: cat
